{
  "name": "@starpower/web-commit-review",
  "version": "1.0.5",
  "description": "A web-based git commit review tool for browsing branches and commits",
  "main": "lib/server.js",
  "type": "module",
  "bin": {
    "web-commit-review": "./bin/web-commit-review-electron.js"
  },
  "scripts": {
    "start": "node bin/web-commit-review-electron.js",
    "start:browser": "node bin/web-commit-review.js",
    "electron": "electron lib/electron-main.js",
    "dev": "nodemon bin/web-commit-review.js",
    "build": "vite build",
    "preview": "vite preview"
  },
  "files": [
    "bin/",
    "lib/",
    "public/",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "git",
    "review",
    "commits",
    "branches",
    "development-tool",
    "cli",
    "worktree",
    "electron"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/web-commit-review.git"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "electron": "^38.1.1",
    "express": "^4.18.2",
    "open": "^10.2.0",
    "simple-git": "^3.19.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
